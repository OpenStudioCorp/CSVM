{
  "name": "cusp",
  "displayName": "CubeScript",
  "publisher": "OpenStudio",
  "description": "the official OpenStudio CubeScript extention",
  "version": "0.0.1",
  "icon": "csvm.ico",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cusp",
      "aliases": ["CubeScript", "cusp"],
      "extensions": [".cusp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cusp",
      "scopeName": "source.cusp",
      "path": "./syntaxes/cusp.tmLanguage.json"
    }],
    "snippets": [{
      "language": "cusp",
      "path": "./snippets/snippets.json"
    }]
  }
}